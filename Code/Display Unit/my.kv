<Button>:
        background_color: 1, 1, 1, 0.7
        border: 16,16,16,16
        markup: True
        font_name: "Rajdhani-Bold"
        background_normal: 'frame.png'
        background_down: 'frame.png'

<MainWindow>: 
        FloatLayout:
                id: Box
                size: self.parent.size

                canvas:
                        Rectangle:
                                pos: self.pos
                                size: self.size
                                source : "bg1.jpg"


                Button:
                        id: c3
                        pos_hint: {"x":0.02, "y":0.02}  
                        size_hint : (0.47,0.47) 
                        text: "[sup][size=100]SF-1 [/size][/sup][size=300]28[/size]" 
                        on_press: root.manager.current = 'Analysis'
                
                Button:
                        id: c4
                        pos_hint: {"x":0.51, "y":0.02}  
                        size_hint : (0.47,0.47) 
                        text: "[sup][size=100]SF-1 [/size][/sup][size=300]28[/size]"
                        on_press: root.manager.current = 'Analysis'

                Button:
                        id: c1
                        pos_hint: {"x":0.02, "y":0.51}  
                        size_hint : (0.47,0.47) 
                        text: "[sup][size=100]SF-1 [/size][/sup][size=300]28[/size]"
                        on_press: root.manager.current = 'Analysis'

                Button:
                        id: c2
                        pos_hint: {"x":0.51, "y":0.51}  
                        size_hint : (0.47,0.47) 
                        text: "[sup][size=100]SF-1 [/size][/sup][size=300]28[/size]"
                        on_press: root.manager.current = 'Analysis'

<AnalysisScreen>:
        FloatLayout:
                id: imgLay
                size: self.parent.size

                canvas:
                        Rectangle:
                                pos : self.pos
                                source : 'da1.png'
                                size : self.size
